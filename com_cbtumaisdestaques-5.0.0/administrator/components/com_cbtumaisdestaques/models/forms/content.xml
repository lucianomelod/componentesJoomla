<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1" default="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" label="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_CREATED_BY" description="COM_CBTUMAISDESTAQUES_FORM_DESC_CONTENT_CREATED_BY" hidden="true" hint="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" label="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_MODIFIED_BY" description="COM_CBTUMAISDESTAQUES_FORM_DESC_CONTENT_MODIFIED_BY" hidden="true" hint="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_MODIFIED_BY"/>
    <field name="title" filter="safehtml" type="text" label="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_TITLE" description="COM_CBTUMAISDESTAQUES_FORM_DESC_CONTENT_TITLE" hidden="true" hint="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_TITLE"/>
    <field name="catid" type="foreignkey" label="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_CATID" description="COM_CBTUMAISDESTAQUES_FORM_DESC_CONTENT_CATID" class="inputbox" input_type="list" table="#__categories" key_field="id" value_field="title" default="0"/>
    <field name="created" default="0000-00-00 00:00:00" type="calendar" class="inputbox" format="%Y-%m-%d %H:%M:%S" showtime="true" translateformat="true" label="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_CREATED" description="COM_CBTUMAISDESTAQUES_FORM_DESC_CONTENT_CREATED" hidden="true" hint="COM_CBTUMAISDESTAQUES_FORM_LBL_CONTENT_CREATED"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
